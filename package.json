{
  "name": "os-remote",
  "author": "LostQuasar",
  "version": "1.0",
  "dependencies": {
    "pebble-clay": "^1.0.4"
  },
  "pebble": {
    "displayName": "OS Remote",
    "uuid": "5e8eec99-fa47-4edf-997c-e28d9b3c5c35",
    "sdkVersion": "3",
